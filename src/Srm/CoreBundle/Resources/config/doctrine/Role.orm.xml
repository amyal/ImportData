<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Srm\CoreBundle\Entity\Role" table="vr_role">
    <id name="roleId" type="integer" column="ROLE_ID">
      <generator strategy="AUTO"/>
    </id>
    <field name="label" type="string" column="LABEL" length="50"/>
    <field name="roleType" type="string" column="ROLE_TYPE" length="10"/>
    <field name="enabled" type="boolean" column="ENABLED"/>
    <field name="deleted" type="boolean" column="DELETED"/>
  </entity>
</doctrine-mapping>
