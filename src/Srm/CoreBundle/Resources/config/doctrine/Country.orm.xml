<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Srm\CoreBundle\Entity\Country" table="vr_country">
    <id name="countryId" type="integer" column="COUNTRY_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="code" type="string" column="CODE" length="3" nullable="false"/>
    <field name="alpha2" type="string" column="ALPHA2" length="2" nullable="false"/>
    <field name="alpha3" type="string" column="ALPHA3" length="3" nullable="false"/>
    <field name="label_an" type="string" column="LABEL_AN" length="45" nullable="false"/>
    <field name="label" type="string" column="LABEL" length="45" nullable="false"/>
  </entity>
</doctrine-mapping>
