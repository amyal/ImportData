<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Srm\CoreBundle\Entity\LegalForm" table="V_LEGAL_FORM">
    <id name="legalFormId" type="integer" column="LEGAL_FORM_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="NAME" length="100" nullable="false"/>
    <field name="natureCapital" type="float" column="NATURE_CAPITAL" nullable="false"/>
    <field name="amountCapital" type="float" column="AMOUNT_CAPITAL" nullable="false"/>
    <field name="ca" type="float" column="CA" nullable="false"/>
    <field name="netIncome" type="float" column="NET_INCOME" nullable="false"/>
    <field name="heritageAssets" type="float" column="HERITAGE_ASSETS" nullable="false"/>
    <field name="heritageCapital" type="float" column="HERITAGE_CAPITAL" nullable="false"/>
    <field name="heritageDebt" type="float" column="HERITAGE_DEBT" nullable="false"/>
    <field name="creationDate" type="datetime" column="CREATION_DATE" nullable="true"/>
    <field name="modificationDate" type="datetime" column="MODIFICATION_DATE" nullable="true"/>
    <field name="enabled" type="boolean" column="ENABLED" nullable="true"/>
    <field name="deleted" type="boolean" column="DELETED" nullable="true"/>
    <many-to-one field="organisation" target-entity="Organisation">
      <join-columns>
        <join-column name="ORGANISATION_ID" referenced-column-name="ORGANISATION_ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
