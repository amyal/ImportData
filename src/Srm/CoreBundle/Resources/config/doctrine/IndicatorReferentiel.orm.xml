<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Srm\CoreBundle\Entity\IndicatorReferentiel" table="VJ_INDICATOR_REFERENTIEL">
    <id name="indicatorReferentielId" type="integer" column="INDICATOR_REFERENTIEL_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="indicatorLevel1" target-entity="IndicatorLevel1">
      <join-columns>
        <join-column name="INDICATOR_LEVEL1_ID" referenced-column-name="INDICATOR_LEVEL1_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="indicatorLevel3" target-entity="IndicatorLevel3">
      <join-columns>
        <join-column name="INDICATOR_LEVEL3_ID" referenced-column-name="INDICATOR_LEVEL3_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="indicatorLevel2" target-entity="IndicatorLevel2">
      <join-columns>
        <join-column name="INDICATOR_LEVEL2_ID" referenced-column-name="INDICATOR_LEVEL2_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="referentiel" target-entity="Referentiel">
      <join-columns>
        <join-column name="REFERENTIEL_ID" referenced-column-name="REFERENTIEL_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="indicator" target-entity="Indicator">
      <join-columns>
        <join-column name="INDICATOR_ID" referenced-column-name="INDICATOR_ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
