<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Srm\CoreBundle\Entity\SitePole" table="VJ_SITE_POLE">
    <id name="sitePoleId" type="integer" column="SITE_POLE_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="site" target-entity="Site">
      <join-columns>
        <join-column name="SITE_ID" referenced-column-name="SITE_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="pole" target-entity="Pole">
      <join-columns>
        <join-column name="POLE_ID" referenced-column-name="POLE_ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
