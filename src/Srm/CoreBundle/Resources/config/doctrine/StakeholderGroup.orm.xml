<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Srm\CoreBundle\Entity\StakeholderGroup" table="V_STAKEHOLDER_GROUP">
    <id name="stakeholderGroupId" type="integer" column="STAKEHOLDER_GROUP_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="NAME" length="25" nullable="false"/>
    <field name="type" type="string" column="TYPE" length="50" nullable="false"/>
    <field name="archetype" type="string" column="ARCHETYPE" length="50" nullable="false"/>
    <field name="creationDate" type="datetime" column="CREATION_DATE" nullable="false"/>
    <field name="modificationDate" type="datetime" column="MODIFICATION_DATE" nullable="true"/>
    <field name="contact" type="string" column="CONTACT" length="25" nullable="false"/>
  </entity>
</doctrine-mapping>
