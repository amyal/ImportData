<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Srm\CoreBundle\Entity\Shareholder" table="V_SHAREHOLDER">
    <id name="shareholderId" type="integer" column="SHAREHOLDER_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="NAME" length="255" nullable="false"/>
    <field name="parts" type="integer" column="PARTS" nullable="true"/>
    <field name="creationDate" type="datetime" column="CREATION_DATE" nullable="true"/>
    <field name="modificationDate" type="datetime" column="MODIFICATION_DATE" nullable="true"/>
    <field name="contact" type="string" column="CONTACT" length="255" nullable="true"/>
    <field name="enabled" type="boolean" column="ENABLED" nullable="true"/>
    <field name="deleted" type="boolean" column="DELETED" nullable="true"/>
    <many-to-one field="contact2" target-entity="Contact">
      <join-columns>
        <join-column name="CONTACT_ID" referenced-column-name="CONTACT_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="legalForm" target-entity="LegalForm">
      <join-columns>
        <join-column name="LEGAL_FORM_ID" referenced-column-name="LEGAL_FORM_ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
