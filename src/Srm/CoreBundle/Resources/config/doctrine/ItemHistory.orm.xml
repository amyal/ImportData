<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Srm\CoreBundle\Entity\ItemHistory" table="V_ITEM_HISTORY">
    <id name="itemHistoryId" type="integer" column="ITEM_HISTORY_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="NAME" length="50" nullable="false"/>
    <field name="themeDescription" type="string" column="THEME_DESCRIPTION" length="255" nullable="false"/>
    <field name="enabled" type="boolean" column="ENABLED" nullable="true"/>
    <field name="deleted" type="boolean" column="DELETED" nullable="true"/>
    <field name="contact" type="string" column="CONTACT" length="50" nullable="false"/>
    <field name="creationDate" type="datetime" column="CREATION_DATE" nullable="false"/>
  </entity>
</doctrine-mapping>
