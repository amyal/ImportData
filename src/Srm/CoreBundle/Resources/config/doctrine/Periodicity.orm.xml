<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Srm\CoreBundle\Entity\Periodicity" table="VR_PERIODICITY">
    <id name="periodicityId" type="integer" column="PERIODICITY_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="label" type="string" column="LABEL" length="20" nullable="true"/>
    <field name="description" type="string" column="DESCRIPTION" length="250" nullable="true"/>
    <field name="enabled" type="boolean" column="ENABLED" nullable="true"/>
    <field name="deleted" type="boolean" column="DELETED" nullable="true"/>
  </entity>
</doctrine-mapping>
