<!DOCTYPE html>
<html>
    <head>
        <title>{% block title %}SRM{% endblock %}</title>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        {% block stylesheets %}
            <link href="{{ asset('bundles/srmwebsite/css/main.css') }}" rel="stylesheet" />
            <link href="{{ asset('bundles/srmwebsite/vendor/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet" />
        {% endblock %}
    </head>
    <body>
        {% block body '' %}

        {% block javascripts %}
            <script src="{{ asset('bundles/srmwebsite/vendor/jquery-2.0.3.min.js') }}"></script>
            <script src="{{ asset('bundles/srmwebsite/vendor/bootstrap/js/bootstrap.min.js') }}"></script>
        {% endblock %}
    </body>
</html>
